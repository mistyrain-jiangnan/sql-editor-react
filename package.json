{
  "name": "sql-editor-react",
  "private": false,
  "version": "0.8.18",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": ".dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
